#include <stdio.h>
#include <string.h>
#include <stdbool.h>

int main(void) {
    char s[200005];

    if (!fgets(s, sizeof(s), stdin)) return 0;

   
    size_t n = strlen(s);
    while (n > 0 && (s[n-1] == '\n' || s[n-1] == '\r' || s[n-1] == ' ' || s[n-1] == '\t'))
        s[--n] = '\0';

    bool dp1[10] = {0};      // 1 цифра
    bool dp2[100] = {0};     // 2 цифры (00..99)
    bool dp3[1000] = {0};    // 3 цифры (000..999)

    for (size_t i = 0; i < n; i++) {
        char c = s[i];
        if (c < '0' || c > '9') continue; 
        int d = c - '0';

        
        for (int x = 0; x < 100; x++) {
            if (dp2[x]) {
                dp3[x * 10 + d] = true;
            }
        }

       
        for (int a = 0; a < 10; a++) {
            if (dp1[a]) {
                dp2[a * 10 + d] = true;
            }
        }

        
        dp1[d] = true;
    }

    int count = 0;
    for (int num = 100; num <= 999; num++) {
        if (dp3[num]) count++;
    }

    printf("%d\n", count);
    return 0;
}

